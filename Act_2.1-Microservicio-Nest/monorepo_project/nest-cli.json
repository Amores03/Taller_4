{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/monorepo_project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/monorepo_project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/monorepo_project",
  "projects": {
    "monorepo_project": {
      "type": "application",
      "root": "apps/monorepo_project",
      "entryFile": "main",
      "sourceRoot": "apps/monorepo_project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepo_project/tsconfig.app.json"
      }
    },
    "message_app": {
      "type": "application",
      "root": "apps/message_app",
      "entryFile": "main",
      "sourceRoot": "apps/message_app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/message_app/tsconfig.app.json"
      }
    }
  }
}